{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "dd-MM-yyyy HH:mm:ss"
    }
  },
  "Server": {
    "Port": 5050,
    "UseHttps": false
  },
  "FileAnalysis": {
    "MaxSearchDepth": 10,
    "AllowedExtensions": [".txt", ".md", ".log", ".csv", ".json", ".xml"]
  }
}
